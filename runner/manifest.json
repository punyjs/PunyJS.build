{
    "name": "trujs-build"
    , "description": [
        ""
    ]
    , "strict": true
    , "repos": [
        {
            "type": "git"
            , "url": "https://github.com/trujs/TruJS.core.git"
            , "branch": "master"
            , "local": "TruJS/core"
        }
    ]
    , "entries": [
        {
            "type": "ioc.monolith"
            , "base": [
                "{TruJS.build.handlers}/builtin-handlers.dtree.json"
                , "{TruJS.build.handlers.ioc}/build-handler.dtree.json"
                , "{TruJS.build.handlers.express}/build-handler.dtree.json"
            ]
            , "config": {
                "dtreeFileName": "builder-dtree.js"
                , "appFileName": "builder-node.js"
                , "engine": "node"
                , "output": "TruJS/builder/"
            }
        }
    ]
}