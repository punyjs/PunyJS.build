{
    "fileExtEncodingMap": {
        "$default": "utf8"
        , ".jpg": "raw"
        , ".png": "raw"
        , ".gif": "raw"
    }
    , "base64EncodeMap": {
        ".png": "image/png"
        , ".jpg": "image/jpeg"
        , ".jpeg": "image/jpeg"
        , ".gif": "image/gif"
        , ".bmp": "image/bmp"
    }
    , "sourceDirectory": "source"
    , "pathsPropertyName": "paths"
    , "docEntryPropertyName": "docs"
    , "namingPropertyName": "naming"
    , "dtreeName": "dtree.json"
    , "appFileName": "app.js"
    , "jsDocExt": [
        ".js"
    ]
    , "extSuffixExceptions": [
        ".js"
        , ".json"
    ]
    , "config": {
        "buildsDirectory": "build"
        , "excludePaths": [
            "*manifest.json"
            , "*dtree.json"
        ]
    }
}